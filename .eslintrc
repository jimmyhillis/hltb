{
  "extends": [
    "airbnb/base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "arrow-body-style": ["error", "always"],
    "no-shadow": [2, {"allow": ["err", "callback"]}],
    "no-unused-vars": [2, {"vars": "all", "varsIgnorePattern": "junk"}],
    "import/prefer-default-export": [0],
    "prettier/prettier": "error"
  }
}
